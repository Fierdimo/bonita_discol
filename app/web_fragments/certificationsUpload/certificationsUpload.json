        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "\treturn {",
        "\t\tcopiaCedulaDocumentInput: $data.chargedData.map( it => {return it.cedula}),",
        "\t\tcertificadoPoliciaDocumentInput: $data.chargedData.map( it => {return it.certPolicia}),",
        "\t\tcertificadoContraloriaDocumentInput: $data.chargedData.map( it => {return it.certContraloria}),",
        "\t\tcertificadoAcademicoBachillerDocumentInput: $data.chargedData.map( it => {return it.certBachiller}),",
        "\t\tcertificadoAcademicoSuperiorDocumentInput: $data.chargedData.map( it => {return it.certSuperior}),",
        "\t\tcertificadoRUTDocumentInput: $data.chargedData.map( it => {return it.RUT}),",
        "\t\tadditionalDataInput: $data.chargedData.map( it => {return it.additionalData}),",
        "\t\tresponsible: $data.session.user_name",
        "\t};"
      ],
      "exposed" : false
