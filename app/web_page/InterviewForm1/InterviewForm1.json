        "../{{context.curricullums_ref.link}}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "\treturn {",
        "\t\t//map interview variable to expected task contract input",
        "\t\tinterviewInput: $data.chargedData.map( it => ({",
        "\t\t\tapplicantPersistenceId: $data.interview.persistenceId,",
        "\t\t\tid: it.interviewData.id,",
        "\t\t\tpostulation: it.interviewData.postulation,",
        "\t\t\tage: it.interviewData.age,",
        "\t\t\tisPreviusEmployee: it.interviewData.isPreviusEmployee,",
        "\t\t\tisHaveVehicle: it.interviewData.isHaveVehicle,",
        "\t\t\tschoolarLevelRequired: it.interviewData.schoolarLevelRequired,",
        "\t\t\texperienceRequired: it.interviewData.experienceRequired,",
        "\t\t\tcompetencesRequired: it.interviewData.competencesRequired,",
        "\t\t\ttechKnowledge: it.interviewData.techKnowledge,",
        "\t\t\tadaptation: it.interviewData.adaptation,",
        "\t\t\tobservations: it.interviewData.observations,",
        "\t\t\tisSelected: it.interviewData.isSelected,",
        "\t\t\tinterviewer: $data.session.user_name",
        "\t\t})),",
        "\t\tresponsible: $data.session.user_name",
        "\t}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "interview" : {
      "type" : "url",
      "value" : [
        "../{{context.interview_ref.link}}"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "chargedData" : {
      "type" : "expression",
      "value" : [
        "return $data.curricullums.map((curr, index) =>{",
        "    curr.document = $data.context.document_ref[index];",
        "    curr.interviewData = {",
        "        applicantPersistenceId: \"\",",
        "        id: \"\",",
        "        postulation: \"\",",
        "        age: 0,",
        "        isPreviusEmployee: false,",
        "        isHaveVehicle: false,",
        "        schoolarLevelRequired: \"\",",
        "        experienceRequired: \"\",",
        "        competencesRequired: \"\",",
        "        techKnowledge: \"\",",
        "        adaptation: \"\",",
        "        observations: \"\",",
        "        isSelected: false,",
        "        interviewer: \"\"",
        "    }",
        "    ",
        "    return curr",
        "})"
      ],
      "exposed" : false
    },
    "postulationType" : {
      "type" : "json",
      "value" : [
        "[\"interna\", \"externa\"]"
      ],
      "exposed" : false
    },
    "session" : {
