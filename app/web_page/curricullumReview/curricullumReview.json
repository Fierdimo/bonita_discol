  "variables" : {
    "task" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}"
      ],
      "exposed" : false
    },
    "curricullums" : {
      "type" : "url",
      "value" : [
        "../{{context.curricullums_ref.link}}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.curricullums ){",
        "\treturn {",
        "\t\t//map curricullums variable to expected task contract input",
        "\t\tcurricullumsInput: $data.chargedData.map( it => ({",
        "\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\tapplicantName: it.applicantName !== undefined ? it.applicantName : null,",
        "\t\t\tapplicantMidname: it.applicantMidname !== undefined ? it.applicantMidname : null,",
        "\t\t\tapplicantLastname: it.applicantLastname !== undefined ? it.applicantLastname : null,",
        "\t\t\tfoundBy: it.foundBy !== undefined ? it.foundBy : null,",
        "\t\t\tdocument: it.document !== undefined ? JSON.stringify(it.document) : null",
        "\t\t}))",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "fileList" : {
      "type" : "expression",
      "value" : [
        "return $data.curricullums.map(curriculum =>{",
        "    return JSON.parse(curriculum.document) ",
        "})"
      ],
      "exposed" : false
    },
    "chargedData" : {
      "type" : "expression",
      "value" : [
        "return $data.curricullums.map((curr, index) =>{",
        "    curr.document = curr.document? JSON.parse(curr.document) : $data.context.document_ref[index];",
        "    return curr",
        "})"
      ],
      "exposed" : false
    }
  },
